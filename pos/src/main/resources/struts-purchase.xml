<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">


<struts>
    <package name="purchase" extends="default" namespace="/purchase">
        <default-interceptor-ref name="crudStack"/>
        <default-action-ref name="home"/>
        <action name="save" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doSave">
            <result type="redirect">/purchase/</result>
        </action>
        <action name="edit" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doEdit">
            <result name="input">/purchase/new.jsp</result>
        </action>
        <action name="update" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doUpdate"/>
        <action name="new" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doNew">
            <result name="input">/purchase/new.jsp</result>
        </action>
         <action name="home" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doHome">
            <result name="success">/purchase/home.jsp</result>
        </action>
        <action name="list" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doList">
            <result name="success">/purchase/list.jsp</result>
        </action>
        <action name="suspend" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doSuspend">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="unSuspend" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doUnSuspend">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="cancel" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doCancel">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="suspended" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doListSuspended">
            <result name="success">/purchase/listSuspended.jsp</result>
        </action>
        <action name="load" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doLoad">
            <result name="success">/purchase/newsale.jsp</result>
        </action>
        <action name="addItem" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doAddItem">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="editItem" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doEditItem">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="deleteItem" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doDeleteItem">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="addPayment" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doAddPayment">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="deletePayment" class="com.chandra.pos.web.purchase.action.PurchaseAction"
                method="doDeletePayment">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="addShipping" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doAddShipping">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="addCustomer" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doAddCustomer">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="deleteCustomer" class="com.chandra.pos.web.purchase.action.PurchaseAction"
                method="doDeleteCustomer">
            <result name="success">/purchase/new.jsp</result>
        </action>
        <action name="reload" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doDeleteItem">
            <result name="success">/purchase/reloadpurchase.jsp</result>
        </action>
        <action name="complete" class="com.chandra.pos.web.purchase.action.PurchaseAction" method="doComplete">
            <result name="success">/purchase/new.jsp</result>
        </action>
    </package>
</struts>
